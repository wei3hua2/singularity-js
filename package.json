{
  "name": "singnet-js",
  "version": "0.0.1",
  "description": "Javascript library for singularitynet",
  "main": "dist/singularity.min.js",
  "scripts": {
    "watch": "webpack --watch --config webpack.config.js",
    "build": "webpack --config webpack.config.js",
    "build-ts": "npm run clean & tsc",
    "clean": "rm -rf build dist",
    "doc-md": "cp markdown-tmpl/partials/index.hbs node_modules/typedoc-plugin-markdown/dist/theme/partials/index.hbs && typedoc --options ./typedoc.json",
    "doc-html": "rm -rf docs-html && typedoc --options ./typedoc-html.json",
    "test-unit": "mocha -r ts-node/register test/unit/**.ts",
    "test-node": "mocha -r ts-node/register test/node/**.ts --timeout 15000",
    "test-browser": "node_modules/karma/bin/karma start karma.conf.js",
    "postinstall": "scripts/install.js",
    "uninstall": "scripts/uninstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wei3hua2/singularity-js.git"
  },
  "author": "James Chong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wei3hua2/singularity-js/issues"
  },
  "homepage": "https://github.com/wei3hua2/singularity-js#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.3",
    "buffer": "^5.2.1",
    "eventemitter3": "^3.1.0",
    "grpc-web-client": "^0.6.3",
    "js-base64": "^2.5.1",
    "protobufjs": "^6.8.8",
    "singularitynet-platform-contracts": "^0.2.6",
    "singularitynet-token-contracts": "^2.0.1",
    "web3-core-promievent": "^1.0.0-beta.46"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/js-base64": "^2.3.1",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.5.2",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "execution-time": "^1.3.0",
    "jasmine-core": "^3.3.0",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^2.0.1",
    "karma-webpack": "^3.0.5",
    "mocha": "^5.2.0",
    "parcel-bundler": "^1.11.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^6.0.5",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "typedoc-plugin-markdown": "^1.1.26",
    "typescript": "^2.8.3",
    "web3": "^1.0.0-beta.46",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  }
}
