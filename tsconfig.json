{
  "include": ["src/**.ts"],
  "exclude": ["node_modules", "test/**.ts"],
  "compilerOptions": {
    "lib": ["es5","dom", "es2017"],
    "types":[],
    "typeRoots":[],
    "sourceMap": true,
    "outDir":"dist",
    "skipLibCheck": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "es5",
    "moduleResolution": "node",
    "diagnostics": true,
    "extendedDiagnostics": true,
    "declaration": true,
    "strict": false,
    "noImplicitAny": false,
    "listFiles": false,
    "listEmittedFiles":false
  }
}
